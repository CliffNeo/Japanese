<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.japanese"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.japanese.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.japanese.TestActivity" />
        <activity android:name="com.example.japanese.ListActivity" />
        <activity android:name="com.example.useless.ListClickActivity" />
        <activity android:name="com.example.practice.SetPracticeActivity" />

        <provider
            android:name="com.example.japanese.DatabaseProvider"
            android:authorities="com.example.japanese.provider"
            android:exported="true" >
        </provider>

        <activity android:name="com.example.practice.SelectActivity" />
        <activity android:name="com.example.practice.PracticeListActivity" />
        <activity android:name="com.example.useless.PracticeTestActivity" />
    </application>

</manifest>